  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WeatherApp
	pod 'Alamofire', '= 5.0.0-rc.2'
	pod 'ObjectMapper'
	pod 'AlamofireObjectMapper'
	pod 'GooglePlaces'
	pod 'GoogleMaps', '5.1.0'
end
